{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "class-methods-use-this": "off",
        "comma-dangle": ["error", "always-multiline"],
        "quotes": ["warn", "single"],
        "no-console": "off",
        "no-plusplus": "off",
        "no-unused-vars": "warn"
    }
}